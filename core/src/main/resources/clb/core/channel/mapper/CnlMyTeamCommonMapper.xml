    <select id="queryMember" parameterType="clb.core.channel.dto.CnlChannelContract" resultMap="ChannelContractMap" >
        SELECT
			*
		FROM
			fms_cnl_channel_contract fccc
		WHERE
			fccc.PARTY_TYPE = #{partyType}
		AND fccc.PARTY_ID = #{partyId}
    </select>
    
    <select id="queryFollow" resultMap="OrdCommissionMap" >
        SELECT
			foc.*,foo.ORDER_TYPE
		FROM
			fms_ord_commission foc,
			fms_ord_order foo
		WHERE
			foc.COMPANY_TYPE = #{ordCommission.companyType, jdbcType = VARCHAR}
		AND foc.COMPANY_ID = #{ordCommission.companyId, jdbcType = DECIMAL}
		AND foc.ORDER_ID = foo.ORDER_ID
		AND foo. STATUS IN
		<foreach collection="followingStatus" item="status" index="index"
            open="(" close=")" separator=",">
            #{status.value, jdbcType = VARCHAR}
        </foreach>
        <if test = "orderType != null">
		AND foo.ORDER_TYPE = #{orderType, jdbcType = VARCHAR}
		</if>
    </select>
    
    <select id="queryAllOrderData" resultMap="OrderMap" >
        SELECT
			foo.*
		FROM
			fms_ord_order foo
		WHERE
			EXISTS (
				SELECT
					1
				FROM
					fms_cnl_channel_contract fccc
				WHERE
					(
						(
							fccc.PARTY_TYPE = #{companyType}
							AND fccc.PARTY_ID = #{companyId}
						)
						<if test = "companyType != null and companyType == 'CHANNEL'.toString()">
							OR foo.CHANNEL_ID = #{companyId}
						</if>
						
					)
				AND fccc.CHANNEL_ID = foo.CHANNEL_ID
			)
    </select>
    
    
    <select id="queryReceivableData" resultMap="ReceiveableMap" >
    	SELECT
			ffr.PAYMENT_COMPANY_TYPE,
			ffr.PAYMENT_COMPANY_ID,
			ffr.RECEIVE_COMPANY_TYPE,
			ffr.RECEIVE_COMPANY_ID,
			ffr.VERSION,
			sum(ffr.HKD_AMOUNT) HKD_AMOUNT
		FROM
			fms_fet_receivable ffr
		WHERE
			ffr.RECEIVE_COMPANY_ID = #{receivable.receiveCompanyId, jdbcType = VARCHAR}
		AND ffr.RECEIVE_COMPANY_TYPE = #{receivable.receiveCompanyType, jdbcType = VARCHAR}
		AND ffr.RECEIPT_TYPE = #{receivable.receiptType, jdbcType = VARCHAR}
		AND ffr.VERSION = (
			SELECT
				max(ffr1.VERSION)
			FROM
				fms_fet_receivable ffr1
			WHERE
				ffr1.RECEIVE_COMPANY_ID = ffr.RECEIVE_COMPANY_ID
			AND ffr1.RECEIVE_COMPANY_TYPE = ffr.RECEIVE_COMPANY_TYPE
			AND ffr1.RECEIPT_TYPE = ffr.RECEIPT_TYPE
			AND ffr1.PAYMENT_COMPANY_TYPE = ffr.PAYMENT_COMPANY_TYPE
			AND ffr1.PAYMENT_COMPANY_ID = ffr.PAYMENT_COMPANY_ID
		)
		AND ffr.ORDER_ID IN
		<foreach collection="orders" item="order" index="index"
            open="(" close=")" separator=",">
            #{order.orderId, jdbcType = DECIMAL}
        </foreach>
		GROUP BY
			ffr.PAYMENT_COMPANY_TYPE,
			ffr.PAYMENT_COMPANY_ID,
			ffr.RECEIVE_COMPANY_TYPE,
			ffr.RECEIVE_COMPANY_ID,
			ffr.VERSION
    </select>
    
    
    <select id="queryOrderDataByChannelId" parameterType="clb.core.order.dto.OrdOrder" resultMap="OrderMap" >
    	SELECT
			foo.*, fcc.CHANNEL_NAME
		FROM
			fms_ord_order foo,
			fms_cnl_channel fcc
		WHERE
			foo.CHANNEL_ID = #{channelId}
		AND foo.CHANNEL_ID = fcc.CHANNEL_ID
		AND foo. STATUS IN (
			'POLICY_EFFECTIVE',
			'ISSUE_SUCCESS',
			'BUY_SUCCESS'
		)
	</select>
    
    
    
    
